Sample Input 0

admin
Sample Output 0

welcome admin
Sample Input 1

prakash
Sample Output 1

welcome prakash

import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String s=sc.nextLine();
        System.out.println("welcome "+s);
        
    }
}


